{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "rootDir": "typings",
    "moduleResolution": "nodenext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
